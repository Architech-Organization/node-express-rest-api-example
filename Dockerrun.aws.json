{
	"AWSEBDockerrunVersion": 2,

	"containerDefinitions": [{
		"name": "node-express-rest-api",
		"image": "node:16",
		"essential": true,
        "entryPoint": [
            "entrypoint.sh"
        ],
		"memory": 128,
		"portMappings": [{
			"containerPort": 8000,
			"hostPort": 6600
		}],
		"workingDirectory": "/usr/src/app"
	}]
}